<table>
  <thead>
  <tr>
    <th>Text</th>
    <th>Priority</th>
  </tr>
  <tbody>
  <% if @list.items %>
    <% @list.items.each do |item| %>
      <% if !(item.is_complete) %>
        <tr>
          <td colspan="2"><%= item.text %></td>
          <td colspan="2"><%= item.priority %></td>
        </tr>
      <% end %>
    <% end %> 
  <% end %>          
  </tbody>
</table>
<table>
  <thead>
  <tr>
    <th>Text</th>
    <th>Priority</th>
  </tr>
  <tbody>
  <% if @list.items %>
    <% @list.items.each do |item| %>
      <% if !(item.is_complete) %>
        <tr>
          <td><%= item.text %></td>
          <td><%= item.priority %></td>
          <td><%= link_to 'x', item, :class => 'destroy', method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    <% end %> 
  <% end %>          
  </tbody>
</table>